@{ 
	var module = new mojoPortal.Business.Module(Model.Module.Guid);
}

@if (module.ShowTitle)
{ 
	<@Model.Module.TitleElement class="social-links__title moduletitle" data-moduleid="module@(Model.Module.Id)">
	@Model.Module.Title

	@if (Model.Module.IsEditable)
	{
		@Html.Partial("_ModuleLinks")
	}
	</@Model.Module.TitleElement>
}
else if (Model.Module.IsEditable)
{
	@Html.Partial("_ModuleLinks")
}